  },
  "dependencies": {
    "@wdio/eyes.webdriverio": "^2.0.2",
    "@wdio/logger": "^5.4.1"
  },
  "peerDependencies": {
    "@wdio/cli": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
