{
  "name": "@wdio/cli",
  "version": "9.21.1",
  "description": "WebdriverIO testrunner command line interface",
  "author": "Christian Bromann <mail@bromann.dev>",
  "homepage": "https://github.com/webdriverio/webdriverio/tree/main/packages/wdio-cli",
  "license": "MIT",
  "bin": {
    "wdio": "./bin/wdio.js"
