{
  "name": "wdio-config",
  "version": "0.1.0",
  "description": "A helper utility ot parse and validate WebdriverIO options",
  "author": "Christian Bromann <christian@saucelabs.com>",
  "homepage": "https://github.com/webdriverio/webdriverio/packages/wdio-config",
  "license": "MIT",
  "main": "./build/index",
  "engines": {
    "node": ">= 4.8.5"
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rm -rf ./build ./coverage",
    "compile": "babel src/ -d build/",
    "lint": "eslint src test",
    "release": "npm run release:patch",
    "release:patch": "np patch --no-cleanup",
    "release:minor": "np minor --no-cleanup",
    "release:major": "np major --no-cleanup",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch": "npm run compile -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio/webdriverio.git"
  },
  "keywords": [
    "webdriver",
    "wdio",
    "wdio-utility"
  ],
  "bugs": {
    "url": "https://github.com/webdriverio/webdriverio/issues"
  },
  "dependencies": {}
}
