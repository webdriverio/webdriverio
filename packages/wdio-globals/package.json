{
  "name": "@wdio/globals",
  "version": "9.17.0",
  "description": "A helper utility for importing global variables directly",
  "author": "Christian Bromann <mail@bromann.dev>",
  "homepage": "https://github.com/webdriverio/webdriverio/tree/main/packages/wdio-globals",
  "license": "MIT",
  "main": "./build/index.cjs",
  "type": "module",
  "module": "./build/index.js",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    },
    "./types": {
      "types": "./types.d.ts"
    }
  },
  "types": "./build/index.d.ts",
  "typeScriptVersion": "3.8.3",
  "engines": {
    "node": ">=18.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdriverio/webdriverio.git",
    "directory": "packages/wdio-globals"
  },
  "keywords": [
    "webdriver",
    "wdio",
    "wdio-utility"
  ],
  "bugs": {
    "url": "https://github.com/webdriverio/webdriverio/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "expect-webdriverio": "^5.3.4",
    "webdriverio": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "expect-webdriverio": {
      "optional": false
    },
    "webdriverio": {
      "optional": false
    }
  }
}
