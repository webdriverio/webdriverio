{
  "name": "@wdio/lambdatest-service",
  "version": "5.0.0",
  "description": "A WebdriverIO service that manages tunnel and job metadata for LambdaTest.",
  "author": "LambdaTest <keys@lambdatest.com>",
  "homepage": "https://github.com/webdriverio/webdriverio/tree/master/packages/wdio-lambdatest-service",
  "license": "MIT",
  "main": "./build/index",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf ./build",
    "compile": "babel src/ -d build/ --config-file ../../babel.config.js",
    "test": "run-s test:*",
    "test:eslint": "eslint src test",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio/webdriverio.git"
  },
  "keywords": [
