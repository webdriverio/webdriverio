  "bugs": {
    "url": "https://github.com/webdriverio/webdriverio/issues"
  },
  "dependencies": {
    "@wdio/logger": "6.0.0-alpha.0",
    "@lambdatest/node-tunnel": "*",
    "@lambdatest/node-rest-client": "*"
  },
  "peerDependencies": {
    "@wdio/cli": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
