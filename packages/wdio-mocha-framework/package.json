  "types": "./build/index.d.ts",
  "typeScriptVersion": "3.8.3",
  "dependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.28",
    "@wdio/logger": "workspace:9.1.3",
