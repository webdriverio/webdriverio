    "assets"
  ],
  "bugs": {
    "url": "https://github.com/webdriverio/webdriverio/issues"
  },
  "dependencies": {
    "@wdio/logger": "6.0.16",
    "express": "^4.14.0",
    "fs-extra": "^9.0.0",
    "morgan": "^1.7.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "static-server-service.d.ts"
}
